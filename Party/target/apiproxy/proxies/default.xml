<?xml version="1.0" encoding="UTF-8"?><ProxyEndpoint name="default">
  <Description>Swagger test</Description>
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
  </PreFlow>
  <Flows>
    <Flow name="GET /cust">
      <Condition>(proxy.pathsuffix MatchesPath "/cust") and (request.verb = "GET")</Condition>
      <Description/>
      <Request>
        <Step>
          <Name>spikearrest</Name>
        </Step>
        <Step>
          <Name>apiquota</Name>
        </Step>
      </Request>
      <Response/>
    </Flow>
    <Flow name="GET /cust/{custId}">
      <Condition>(proxy.pathsuffix MatchesPath "/cust/{custId}") and (request.verb = "GET")</Condition>
      <Description/>
      <Request>
        <Step>
          <Name>apiquota</Name>
        </Step>
        <Step>
          <Name>spikearrest</Name>
        </Step>
      </Request>
      <Response/>
    </Flow>
  </Flows>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <HTTPProxyConnection>
    <BasePath>/v1/Party</BasePath>
    <VirtualHost>default</VirtualHost>
    <VirtualHost>secure</VirtualHost>
  </HTTPProxyConnection>
  <RouteRule name="default">
    <TargetEndpoint>default</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>